---
layout: none
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Elijah Miller</title>
 <link href="https://thejqr.com/atom.xml" rel="self"/>
 <link href="https://thejqr.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://thejqr.com/</id>
 <author>
   <name>Elijah Miller</name>
   <email>elijah.miller@gmail.com</email>
 </author>

 {% for post in site.posts %}
   <entry>
     <title>{{ post.title }}</title>
     <link href="https://thejqr.com{{ post.url }}"/>
     <updated>{{ post.date | date_to_xmlschema }}</updated>
     <id>http://thejqr.com{{ post.id }}</id>
     <content type="html">{{ post.content | xml_escape }}</content>
   </entry>
 {% endfor %}

</feed>
